{
        "manifestVersion": 1,
        "id": "vsts-extensions-myExtensions",
        "version": "1.0.64",
        "name": "Sprint Retrospective",
        "description": "Sprint Retrospective Extension",
        "publisher": "petdun",
        "targets": [
            {
                "id": "Microsoft.VisualStudio.Services"
            }
        ],
		"scopes": [
			"vso.work"
		],
        "contributions": [
			 {
				"id": "retrospective-hub",
				"type": "ms.vss-web.hub",
				"description": "Adds a hub to the Work hub group",
				"targets": [
					"ms.vss-work-web.work-hub-group"
				],
				"properties": {
					"name": "Retrospective",
					"order": 100,
					"uri": "context.html"
				}
			}
        ],
        "files": [
            {
                "path": "context.html", "addressable": true
            },
			{
                "path": "main.js", "addressable": true
            },
			{
                "path": "bundle.js", "addressable": true
            },
            {
                "path": "sdk/scripts", "addressable": true
            },
			{
			    "path": "css", "addressable": true
			}
        ]

    }