{
        "manifestVersion": 1,
        "id": "vsts-extensions-myExtensions",
        "version": "1.0.1",
        "name": "My First Set of Extensions",
        "description": "Samples containing different widgets extending dashboards",
        "publisher": "peterdunlop",
        "targets": [
            {
                "id": "Microsoft.VisualStudio.Services"
            }
        ],
         "contributions": [
            {
                "id": "HelloWorldWidget",
                "type": "ms.vss-dashboards-web.widget",
                "targets": [
                    "ms.vss-dashboards-web.widget-catalog"
                ],
                "properties": {
                    "name": "Hello World Widget",
                    "description": "My first widget",              
                    "uri": "hello-world.html",
                    "supportedSizes": [
                         {
                                "rowSpan": 1,
                                "columnSpan": 2
                            }
                        ],
                    "supportedScopes": ["project_team"]
                }
            },
			{
				"id": "product-backlog-hub-tab",
				"type": "ms.vss-web.tab",
				"description": "Adds a tab to the backlogs hub",
				"targets": [
					"ms.vss-work-web.product-backlog-tabs" 
				],
				"properties": {
					"uri": "context.html", 
					"title": "Web Context (Backlogs hub)",
					"name": "Web Context Backlog",
					"registeredObjectId": "showContext"
				}
			},
			{
				"id": "iteration-backlog-hub-tab",
				"type": "ms.vss-web.tab",
				"description": "Adds a tab to the iteration hub 2",
				"targets": [
					"ms.vss-work-web.iteration-backlog-tabs"
				],
				"properties": {
					"uri": "context.html",
					"title": "Web Context2(Iteration Hub)",
					"name": "Web Context Iteration Hub",
					"registeredObjectId": "showContext"
				}
			},
			{
				"id": "iteration-product-backlog-hub-tab",
				"type": "ms.vss-web.tab",
				"description": "Adds a tab to the both backlogs and iteration hub",
				"targets": [
					"ms.vss-work-web.product-backlog-tabs",
					"ms.vss-work-web.iteration-backlog-tabs"
				],
				"properties": {
					"uri": "context.html",
					"title": "Web Context (Both hub)",
					"name": "Web Context Both",
					"registeredObjectId": "showContext"
				}
			} 


        ],
        "files": [
            {
                "path": "hello-world.html", "addressable": true
            },
            {
                "path": "sdk/scripts", "addressable": true
            },
			{
			    "path": "css", "addressable": true
			}
        ],
		"scopes":[
			"vso.work"
		]

    }